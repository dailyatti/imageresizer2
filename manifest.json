{
  "name": "ImageFlow Pro - Professional Image Processing SaaS",
  "short_name": "ImageFlow Pro",
  "description": "Advanced image processing with wireless file sharing, QR code pairing, and lossless quality conversion",
  "version": "1.0.0",
  "manifest_version": 3,
  "start_url": "/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": "#667eea",
  "background_color": "#1e1b4b",
  "scope": "/",
  "categories": ["photo", "productivity", "utilities"],
  "lang": "hu",
  "dir": "ltr",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Upload Images",
      "short_name": "Upload",
      "description": "Quickly upload and process images",
      "url": "/?action=upload",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Connect Device",
      "short_name": "Connect",
      "description": "Connect to another device via QR code",
      "url": "/?action=connect",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,PHN2ZyB3aWR0aD0iOTYiIGhlaWdodD0iOTYiIHZpZXdCb3g9IjAgMCAyNCAyNCIgZmlsbD0ibm9uZSIgeG1sbnM9Imh0dHA6Ly93d3cudzMub3JnLzIwMDAvc3ZnIj4KPHBhdGggZD0iTTMgM2g2djZIM3ptOCAwaDZ2NmgtNnptLTggOGg2djZIM3ptMTMgMGgzdjNoLTN6bS0zLTNoM3YzaC0zeiIgZmlsbD0iIzY2N2VlYSIvPgo8L3N2Zz4K",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "capture_links": "new-client",
  "protocol_handlers": [
    {
      "protocol": "web+imageflow",
      "url": "/?protocol=%s"
    }
  ]
}